{
  "name": "ai-voyage",
  "version": "0.5.0",
  "private": "true",
  "description": "AI demonstration",
  "packageManager": "pnpm@7.9.5",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --ext=jsx,js .",
    "lint:fix": "eslint --ext=jsx,js . --fix",
    "format": "prettier --write \"./**/*.{js,jsx}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "eslint-config-prettier": "^8.6.0",
    "next": "^13.1.1",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-use": "^17.4.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "eslint": "^8.31.0",
    "eslint-config-next": "13.1.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.2",
    "tailwindcss": "^3.2.4"
  },
  "lint-staged": {
    "*.jsx,*.js,*.mdx": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "tailwindcss",
    "template",
    "radix",
    "react",
    "nextjs"
  ]
}